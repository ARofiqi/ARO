@Library("sharedLibrary@main") _

pipeline {
    agent any
    stages {
        stage('Step call') {
            steps {
                script {
                    echo hello()
                }
            }
        }
        stage('Hello World') {
            steps {
                script {
                    echo hello.world()
                }
            }
        }
        stage('with param') {
            steps {
                script {
                    echo hello.say("Ahmad Rofiqi")
                }
            }
        }
    }
}
