pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Start Build Project...'
                sh 'npm run build'
                echo 'Finish Build Project...'
            }
        }
        stage('Test') {
            steps {
                echo 'Start Testing..'
                echo 'No Testing file yet'
                // sh 'npm run test'  
                echo 'Finish Testing...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying project....'
            }
        }
    }
}