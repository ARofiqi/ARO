@Library("sharedLibrary@main") _

pipeline {
    agent any
    stages {
        stage('Step call') {
            steps {
                script {
                    echo hello()
                }
            }
        }
        stage('Hello World') {
            steps {
                script {
                    echo hello.world()
                }
            }
        }
        stage('with param') {
            steps {
                script {
                    echo hello.say("Ahmad Rofiqi")
                }
            }
        }
        stage('list param') {
            steps {
                script {
                    hello.say(["Ahmad", "Rofiqi"])
                }
            }
        }
        stage('object param') {
            steps {
                script {
                    hello.person([name: "Ahmad", age: 22])
                }
            }
        }
        stage('config') {
            steps {
                script {
                    // def config = libraryResource("config/build.json")
                    // echo config
                    hello.config()
                }
            }
        }
    }
}
